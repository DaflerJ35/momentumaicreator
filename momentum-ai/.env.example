# Firebase Configuration
VITE_FIREBASE_API_KEY=your_firebase_api_key_here
VITE_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your_project_id
VITE_FIREBASE_STORAGE_BUCKET=your_project.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=your_sender_id
VITE_FIREBASE_APP_ID=your_app_id
VITE_FIREBASE_DATABASE_URL=https://your_project_id.firebaseio.com
VITE_FIREBASE_MEASUREMENT_ID=your_measurement_id

# Stripe Configuration
# Get your publishable key from: Stripe Dashboard -> Developers -> API keys     
VITE_STRIPE_PUBLISHABLE_KEY=your_stripe_publishable_key_here

# Gemini API Configuration
VITE_GEMINI_API_KEY=your_gemini_api_key_here

# Ollama API Configuration
VITE_OLLAMA_API_KEY=your_ollama_api_key_here
VITE_OLLAMA_BASE_URL=http://localhost:11434

# Server AI Configuration
# Set to 'true' to use server-side AI API instead of direct Gemini calls        
# See server/AI_SETUP.md for server configuration details
VITE_USE_SERVER_AI=true
VITE_API_URL=http://localhost:3001

# Multimedia Provider Selectors
# These select which providers to use for multimedia generation (configured on server)
VITE_IMAGE_PROVIDER=dalle3
VITE_VIDEO_PROVIDER=runway
VITE_VOICE_PROVIDER=elevenlabs

# Application Configuration
VITE_APP_ENV=development
